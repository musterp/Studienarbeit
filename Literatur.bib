}


@article{Ucurum.2008,
    abstract = {We have investigated hysteresis in pentacene-based field-effect transistors with SiO(2) as gate dielectric. A clear hysteresis behavior in transfer and output characteristics is reported. Measurements show that the observed hysteresis is due to hole trapping in the pentacene film. Long time constants of the trapping/detrapping mechanism are demonstrated through transient measurements. An initialization routine to be performed prior to measurements is proposed to enable reproducible and reliable measurements on pentacene transistors. (C) 2008 American Institute of Physics. [DOI: 10.1063/1.2999643]},
    author = {Ucurum, C. and Goebel, H. and Yildirim, F. A. and Bauhofer, W. and Krautschneider, W.},
